<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ejercicio 5</title>
    <link rel="stylesheet" href="CSS/estilosTabla.css">
</head>
<body>
    <h1>Productos</h1>
    <div class="contenedor">
    <table>
<thead>
<tr>
    <th >ID</th>
    <th>Nombre</td>
    <th>Cantidad</th>
    <th>Precio</th>
    <th>Codigo</th>
<th>Editar/ Borrar</th>
</tr>

</thead>
<tbody>
    <%
    productosCorrectos.forEach((producto) =>{
    %>
    <tr>
<td><%= producto.idproducto %></td>
<td><%= producto.nombre %></td>
<td><%= producto.cantidad %></td>
<td><%= producto.precio %></td>
<td><%= producto.codprod %></td>
<td>
    <a href="/editarProducto/<%= producto.idproducto %>">Editar / </a>
    <a href="">Borrar</a>
</td>  
</tr>
<%
    })
%>
</tbody>
    </table>
</div>
</body>
</html>